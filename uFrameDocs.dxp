      <TopicCategory Id="CONTENTSNIPPETS" AutoIndex="false">
        <Caption m="2014-10-13T18:12:28.499194Z">Content Snippets</Caption>
        <ChildTopicCategories />
        <Topics>
          <Topic Id="68e5ef59-c521-4583-b817-6401fc77e30a" Status="New Topic" CreatedOn="2014-10-26T16:59:13.8733297Z" ModifiedOn="2014-10-26T16:59:13.8843367Z" PageTypeName="for include.html" AutoIndex="true" HelpContextIds="" Name="ViewBindingsTutorial" BuildFlags="{4AAE0FD4-7313-4FEE-A25F-0785B885F736},{46CEF21D-3301-4517-A875-A0906828390E}">
            <Title />
            <ContentsTitle />
            <Notes></Notes>
